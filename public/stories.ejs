<header class="blog-header">
  <div class="header-fill-overlay flex">
    <div class="container">
      <div class="row">
        <div class="col s12 center-align">
          <p>
            <i class="fa fa-quote-left"></i>
              <span class="snippet"><%-snippet%></span>
            <i class="fa fa-quote-right"></i>
          </p>
          <span class="snippet_author">
            -- <%- snippet_author %>
          </span>
        </div>
      </div>
    </div>
  </div>
</header>

<main>
  <section class="article">
    <div class="container">
      <div class="col s12">
        <article>
          <%- partial('_stories/electronic-condo-proxies', {}) %>
        </article>
      </div>
    </div>
  </section>

</main>
