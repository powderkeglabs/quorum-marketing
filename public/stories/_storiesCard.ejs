<div class="col s12 m6">
  <div class="stories__card card">
    <div class="stories__card-trim"></div>
    <div class="stories__card--content card-content">
      <span class="stories__card--title card-title"><%= title %></span>
      <span class="stories__card--date"><%= date %></span>
      <p><%= text %></p>
    </div>
    <div class="card-action stories__card--action right-align">
      <a href="<%= url %>">Read the Case Study</a>
    </div>
  </div>
</div>
